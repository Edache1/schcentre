<%  let terms = (data.interval) ? data.interval : ''; %>
<div class="container">
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Term</th>
        <th scope="col">Column heading</th>
      </tr>
    </thead>
    <tbody>
      <% if (terms){ terms.forEach(term => {
        let intid = term.intid;%>
      <tr >
        <td><%- term.term %></td>
        <td><a href="/interval/update/<%-intid %>" class="btn btn-info"><i class="fas fa-eye"> </i>details</a></td>
      </tr>
      <% })} %>
    </tbody>
  </table>
</div>
<%- include('../templates/footer') %>